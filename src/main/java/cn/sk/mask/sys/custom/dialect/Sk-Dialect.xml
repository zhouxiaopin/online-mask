<dialect
		xmlns="http://www.thymeleaf.org/extras/dialect"
		prefix="sk"
		namespace-uri="http://www.sk.com/thymeleaf/sk"
		class="cn.sk.mask.sys.custom.dialect.SkDialect">


	<!--标签-->
	<element-processor
		name="aBtn"
		class="cn.sk.mask.sys.custom.tag.AbtnTag">
	</element-processor>
	<element-processor
			name="button"
			class="cn.sk.mask.sys.custom.tag.ButtonTag">
	</element-processor>
	<element-processor
			name="input"
			class="cn.sk.mask.sys.custom.tag.InputTag">
	</element-processor>
	<element-processor
			name="label"
			class="cn.sk.mask.sys.custom.tag.LabelTag">
	</element-processor>
	<element-processor
			name="menuTree"
			class="cn.sk.mask.sys.custom.tag.MenuTreeTag">
	</element-processor>
	<element-processor
			name="navBar"
			class="cn.sk.mask.sys.custom.tag.NavBarTag">
	</element-processor>
	<element-processor
			name="select"
			class="cn.sk.mask.sys.custom.tag.SelectTag">
	</element-processor>
	<element-processor
			name="treeSelect"
			class="cn.sk.mask.sys.custom.tag.TreeSelectTag">
	</element-processor>

</dialect>
